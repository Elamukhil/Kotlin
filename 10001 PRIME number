fun main() {
    var num=1;var count=0;
    while (count < 10001){
      var flag=false
      num=num+1
      for (i in 2..num/2){
        if (num % i == 0) {
            flag = true
          	break
        }
      }
      if (!flag){
        count = count+1;
      }
    }
    println("Value of nth prime is:  $num")
  }
  //Value of 10001 prime is:  104743
